{
  "name": "kayak.rocks",
  "description": "Personal website",
  "version": "1.0.0",
  "private": true,
  "author": "kayak <kayak@kayak.rocks>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "t": "tsc"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": ["yarn lint --fix", "yarn format"]
  },
  "dependencies": {
    "@types/styled-components": "5.1.3",
    "babel-plugin-styled-components": "1.11.1",
    "gatsby": "2.24.67",
    "gatsby-background-image": "1.1.2",
    "gatsby-image": "2.4.20",
    "gatsby-plugin-manifest": "2.4.33",
    "gatsby-plugin-sharp": "2.6.38",
    "gatsby-plugin-styled-components": "3.3.12",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-filesystem": "2.3.32",
    "gatsby-transformer-sharp": "2.5.16",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "styled-components": "5.2.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "twin.macro": "1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/node": "14.11.2",
    "@types/react": "16.9.50",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-plugin-root-import": "6.5.0",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "tailwindcss": "1.8.10",
    "typescript": "4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
