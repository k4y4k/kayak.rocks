{
  "name": "kayak.rocks",
  "description": "Personal website",
  "version": "1.0.0",
  "private": true,
  "author": "kayak <kayak@kayak.rocks>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "yarn eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "yarn prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "t": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
